
@inherits ToolbarComponentBase

<ToolbarComponentWrapper>
    <a class="@CssIcon"
       title="@Title"
       href="@Href"></a>
</ToolbarComponentWrapper>

@code {
    [Parameter]
    public string CssIcon { get; set; } = null!;

    [Parameter]
    public string Title { get; set; } = string.Empty;

    [Parameter]
    public string Href { get; set; } = string.Empty;
}
